        </activity>
        <activity android:name="com.example.dineshbalajivenkataraman.views.MultipleChoice" />
        <activity android:name="com.example.dineshbalajivenkataraman.views.InputText" />
         <activity android:name="com.example.dineshbalajivenkataraman.views.Images" />
        <activity android:name="com.example.dineshbalajivenkataraman.views.CheckBoxDisplay" />
        <activity android:name="com.example.dineshbalajivenkataraman.views.QuizScore"></activity>
    </application>

</manifest>